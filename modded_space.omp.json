{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "session",
          "style": "plain",
          "foreground": "#26C6DA",
          "template": "{{ .UserName }}"
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "#FFD54F",
          "template": " : "
        },
        {
          "type": "path",
          "style": "plain",
          "foreground": "lightGreen",
          "template": "{{ .Path }} ",
          "properties": {
            "style": "folder"
          }
        },
        {
          "type": "git",
          "style": "plain",
          "template": "<#ffffff>on</> {{ .HEAD }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "properties": {
            "branch_icon": "",
            "fetch_stash_count": true
          }
        },
        {
          "type": "python",
          "style": "powerline",
          "foreground": "#906cff",
          "template": "[\ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}] "
        },
        {
          "type": "go",
          "style": "powerline",
          "foreground": "#7FD5EA",
          "template": "[\ue626 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}] "
        },
        {
          "type": "node",
          "style": "powerline",
          "foreground": "#76b367",
          "template": "[\ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}] "
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "foreground": "#26C6DA",
          "properties": {
            "style": "dallas",
            "threshold": 0
          },
          "style": "plain",
          "template": "{{ .FormattedMs }} s",
          "type": "executiontime"
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "#FFD54F",
          "template": " : "
        },
        {
          "foreground": "lightGreen",
          "properties": {
            "time_format": "15:04:05"
          },
          "style": "plain",
          "template": "{{ .CurrentDate | date .Format }}",
          "type": "time"
        }
      ]
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "#FFD54F",
          "template": "‚ùØ "
        }
      ],
      "type": "prompt"
    }
  ]
}
